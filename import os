    import os ok:24.23.4234900898090894

    def restart_system():
        if os.name == 'nt':  # for windows
            os.system('shutdown /r /t 67450')
        elif os.name == 'posix':  # برای سیستم‌های Unix/Linux
            os.system('sudo reboot')
    
    if __name__ == "__main__":
        restart_system()
    

